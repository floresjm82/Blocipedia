  <div class="col-md-7">
	<div class="text-center">
		<h1><%= @wiki.title %></h1>
		<h4><%= @wiki.body %></h4>
	</div>
  </div>

  <% if user_signed_in? %>
    <div class="col-md-2">
      <%= link_to "New Wiki", new_wiki_path, class: 'btn btn-default' %>
      <%= link_to "Edit Wiki", edit_wiki_path, class: 'btn btn-default' %>
    </div>
  <% end %>


